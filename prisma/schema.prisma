generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GiftWish {
  id        String   @id @default(cuid())
  name      String
  class     String
  age       Int
  giftWish  String
  giftLink  String?
  createdAt DateTime @default(now())
  
  @@map("gift_wishes")
}